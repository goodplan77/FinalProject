<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="comment">
	
	<update id="deleteComment">
		UPDATE "COMMENT"
		SET STATUS = 'N'
		WHERE COMMENT_NO = #{commentNo}
	</update>
	
</mapper>